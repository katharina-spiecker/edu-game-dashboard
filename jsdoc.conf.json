{
    "source": {
        "include": ["src/components", "src/views"],
        "exclude": ["node_modules"],
        "includePattern": ".+\\.jsx?$"
    },
    "plugins": ["plugins/markdown"],
    "opts": {
        "destination": "./docs",
        "recurse": true,
        "readme": "./README.md",
        "template": "templates/default"
    }
}
  